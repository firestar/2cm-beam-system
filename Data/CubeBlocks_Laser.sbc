            <BlockTopology>TriangleMesh</BlockTopology>
            <Size x="1" y="1" z="3" />
            <ModelOffset x="0" y="0" z="0" />
            <Model>Models\2cm\Beam System\Generator_1x3.mwm</Model>
            <Components>
                <Component Subtype="SteelPlate" Count="20" />
                <Component Subtype="MetalGrid" Count="15" />
                <Component Subtype="Construction" Count="40" />
                <Component Subtype="LargeTube" Count="5" />
                <Component Subtype="Computer" Count="5" />
                <Component Subtype="SmallTube" Count="20" />
                <Component Subtype="Superconductor" Count="60" />
                <Component Subtype="SteelPlate" Count="20" />
