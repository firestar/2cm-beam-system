            <BuildProgressModels>
                <Model BuildPercentUpperBound="0.2" File="Models\2cm\Beam System\Generator_1x3_Constr1.mwm" />
                <Model BuildPercentUpperBound="0.4" File="Models\2cm\Beam System\Generator_1x3_Constr2.mwm" />
                <Model BuildPercentUpperBound="0.6" File="Models\2cm\Beam System\Generator_1x3_Constr3.mwm" />
                <Model BuildPercentUpperBound="0.8" File="Models\2cm\Beam System\Generator_1x3_Constr4.mwm" />
                <Model BuildPercentUpperBound="1.0" File="Models\2cm\Beam System\Generator_1x3_Constr5.mwm" />
            </BuildProgressModels>
            <BlockPairName>2CM_BS_Laser_Generator</BlockPairName>
            <MirroringX>Z</MirroringX>
            <MirroringZ>X</MirroringZ>
            <EdgeType>Light</EdgeType>
            <Upgrades>
                <MyUpgradeModuleInfo>
                    <UpgradeType>PowerEfficiency</UpgradeType>
                    <Modifier>1.0</Modifier>
                    <ModifierType>Multiplicative</ModifierType>
                </MyUpgradeModuleInfo>
            </Upgrades>
            <!-- <PrimarySound>BlockModuleEfficiency</PrimarySound> -->
            <DamageEffectName>Damage_Electrical_Damaged</DamageEffectName>
            <DamagedSound>ParticleElectrical</DamagedSound>
            <EmissiveColorPreset>Default</EmissiveColorPreset>
            <DestroyEffect>BlockDestroyedExplosion_Large</DestroyEffect>
            <DestroySound>WepSmallWarheadExpl</DestroySound>
            <GuiVisible>false</GuiVisible>
            <IsAirTight>true</IsAirTight>
            <ResourceSourceGroup>Reactors</ResourceSourceGroup>
            <ResourceSinkGroup>BeamGenerator</ResourceSinkGroup>
            <StandbyPowerConsumption>0.0001</StandbyPowerConsumption>
            <OperationalPowerConsumption>6.0</OperationalPowerConsumption>
